@model JavaScriptViewModel
@if (Model == null) { return; }

@if (!string.IsNullOrWhiteSpace(Model.GoogleTagManagerCode))
{
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=@Model.GoogleTagManagerCode"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
}